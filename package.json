{
  "name": "svelte-awesome",
  "version": "0.0.0-development",
  "description": "Font Awesome component for Svelte JS, using inline SVG",
  "main": "dist/svelte-awesome.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobBrazier/svelte-awesome.git"
  },
  "author": "Rob Brazier",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "icons": "node build/generate-icons",
    "example:dev": "poi -c example/poi.config.js",
    "example:build": "poi build -c example/poi.config.js",
    "lint": "eslint ./{example,src}/**/*.{html,js}",
    "test": "npm run lint",
    "commit": "commit",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "keywords": [
    "font-awesome",
    "awesome",
    "svelte",
    "svg"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "verifyConditions": {
      "path": "./build/verify-conditions.js"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.0",
    "@commitlint/config-conventional": "^6.1.0",
    "@commitlint/prompt-cli": "^6.1.0",
    "babel-core": "^6.26.0",
    "cassandra-map": "^0.1.7",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "font-awesome": "^4.7.0",
    "graceful-fs": "^4.1.11",
    "husky": "^0.14.3",
    "lodash": "^4.17.5",
    "patch-module": "^0.1.0",
    "poi": "^10.1.7",
    "poi-preset-svelte": "^9.0.1",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^4.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "semantic-release": "^15.1.5",
    "svelte": "^1.55.0",
    "svgfont2js": "^0.1.2"
  }
}
