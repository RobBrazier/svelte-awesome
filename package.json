{
  "name": "svelte-awesome",
  "version": "3.0.1",
  "description": "Font Awesome component for Svelte JS, using inline SVG",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobBrazier/svelte-awesome.git"
  },
  "author": "Rob Brazier",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "icons": "node .github/scripts/generate-icons",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "npm run lint",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "gh-release": "is-ci && gh-release -y",
    "version:major": "release-it major --ci",
    "version:minor": "release-it minor --ci",
    "version:patch": "release-it patch --ci"
  },
  "keywords": [
    "font-awesome",
    "awesome",
    "svelte",
    "svg"
  ],
  "devDependencies": {
    "regularSvgIconsV5": "npm:@fortawesome/free-regular-svg-icons@^5.10.1",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/fontawesome-common-types": "^6.3.0",
    "@sveltejs/adapter-static": "2.0.1",
    "@sveltejs/kit": "1.9.2",
    "@sveltejs/package": "1.0.2",
    "acorn": "^8.0.2",
    "auto-changelog": "^2.0.0",
    "cassandra-map": "^0.1.7",
    "change-case": "^4.0.0",
    "font-awesome": "^4.7.0",
    "gh-release": "^7.0.2",
    "graceful-fs": "^4.1.11",
    "is-ci": "^3.0.0",
    "lodash": "^4.17.5",
    "npm-run-all": "^4.1.3",
    "patch-module": "^0.1.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "release-it": "^15.0.0",
    "svelte": "^3.46.2",
    "svelte-check": "^2.2.6",
    "svelte-highlight": "^6.0.1",
    "svelte-preprocess": "^5.0.1",
    "svelte2tsx": "^0.5.1",
    "svgfont2js": "^0.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "svelte": "^3.43.1",
    "@fortawesome/fontawesome-svg-core": "*"
  }
}
